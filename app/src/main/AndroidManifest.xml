<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.login22_01_19_h1">

    <!--
     The ACCESS_COARSE/FINE_LOCATION permissions are not required to use
     Google Maps Android API v2, but you must specify either coarse or fine
     location permissions for the "MyLocation" functionality.
-->
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION"/>
    <uses-permission android:name="android.permission.ACCESS_BACKGROUND_LOCATION" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="com.android.alarm.permission.SET_ALARM" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.CAMERA"/>


    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_app_icon_sec_logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Login220119h1"
        android:usesCleartextTraffic="true">
        <activity
            android:name=".MainActivity_Card_Centers"
            android:exported="false" />
        <activity
            android:name=".MainActivityNotification"
            android:exported="false" />

        <activity
            android:name=".Confirmation_Appointment"
            android:exported="false" />
        <activity
            android:name=".MainActivitySpinner"
            android:exported="false"
            tools:ignore="MissingClass" />
        <activity
            android:name=".Menu.Navigation_Main"
            android:exported="false" />
        <activity
            android:name=".FinalPage"
            android:exported="false" />
        <activity
            android:name=".LoginSingup.signUp_mysqlphp"
            android:exported="false" />
        <activity
            android:name=".LoginSingup.Login_MySql"
            android:exported="false" />
        <activity
            android:name=".MainActivityOcr"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <meta-data
            android:name="com.google.android.gms.vision.DEPENDENCIES"
            android:value="ocr" />
        <receiver android:name=".ReminderBreodcast" />
    </application>

</manifest>